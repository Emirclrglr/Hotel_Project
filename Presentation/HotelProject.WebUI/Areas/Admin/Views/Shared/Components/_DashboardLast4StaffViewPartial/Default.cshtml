@model IEnumerable<ResultStaffDto>
<div class="row">
    @foreach(var item in Model){
        <div class="col-lg-3 col-sm-6">
            <div class="card">
                <div class="card-body">
                    <div class="text-center">
                        <img src="@item.staffImageUrl" class="rounded-circle" width="100" height="100" alt="">
                        <h5 class="mt-3 mb-1">@item.staffName</h5>
                        <p class="m-0">@item.staffTitle</p>
                    </div>
                </div>
            </div>
        </div>
    }
</div>
