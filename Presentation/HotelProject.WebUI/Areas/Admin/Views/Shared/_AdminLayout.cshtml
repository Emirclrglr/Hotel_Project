
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
    <!--Head-->
@await Component.InvokeAsync("_AdminHeadPartial")

<body>

    <!--*******************
        Preloader start
    ********************-->
    @await Component.InvokeAsync("_AdminPreloaderPartial")
    <!--*******************
        Preloader end
    ********************-->

    
    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper">

        <!--**********************************
            Nav header start
        ***********************************-->
        @await Component.InvokeAsync("_AdminNavHeaderPartial")
        <!--**********************************
            Nav header end
        ***********************************-->

        <!--**********************************
            Header start
        ***********************************-->
        @await Component.InvokeAsync("_AdminHeaderPartial")
        <!--**********************************
            Header end ti-comment-alt
        ***********************************-->

        <!--**********************************
            Sidebar start
        ***********************************-->
        @await Component.InvokeAsync("_AdminSidebarPartial")
        <!--**********************************
            Sidebar end
        ***********************************-->

        <!--**********************************
            Content body start
        ***********************************-->
        <div class="content-body">


            <!-- row -->
            @* @await Component.InvokeAsync("_AdminBreadcrumbPartial") *@

            <div class="container-fluid">
                @RenderBody()
            </div>
            <!-- #/ container -->
        </div>

        <!--**********************************
            Content body end
        ***********************************-->
        
        
        <!--**********************************
            Footer start
        ***********************************-->
        @await Component.InvokeAsync("_AdminFooterPartial")
        <!--**********************************
            Footer end
        ***********************************-->
    </div>
    <!--**********************************
        Main wrapper end
    ***********************************-->

    <!--**********************************
        Scripts
    ***********************************-->
    @await Component.InvokeAsync("_AdminScriptsPartial")

</body>

</html>